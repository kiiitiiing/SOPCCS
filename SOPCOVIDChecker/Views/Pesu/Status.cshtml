
@{
    ViewData["Title"] = "PESU";
}

    <div class="card card-warning card-outline">
        <div class="card-header">
            <div class="row">
                <div class="col-md-6">
                    <h4>
                        SAMPLES IN @User.FindFirstValue("ProvinceName").ToUpper()<br />
                    </h4>
                    <small class="badge bg-gradient-warning">
                        Total: <span class="ctr"></span>
                    </small>
                </div>
                <div class="col-md-6">
                    <form class="fa-pull-right form-inline ml-3" asp-controller="Pesu" asp-action="StatusPartial" method="get" onsubmit="return partialViewGet(this, '#pesu_status');">
                        <!-- INPUT -->
                        <input class="form-control form-control-sm"
                               name="q"
                               placeholder="Search name"
                               type="text" />
                        <button id="searchPatientBtn" type="submit" class="btn btn-sm btn-success">
                            <i class="fa fa-search"></i>
                        </button>
                    </form>
                </div>
            </div>
        </div>
        <div class="card-body" id="pesu_status">
        </div>
    </div>

<script>
    LoadPartialView('#pesu_status', '@Url.Action("StatusPartial","Pesu")');
</script>